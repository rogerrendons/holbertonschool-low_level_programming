#include <stdlib.h>
#include <string.h>
#include <stdio.h>
#include "lists.h"

/**
 * get_nodeint_at_index - get determinate node.
 * @head: Head of linked list.
 * @index: Item of node
 * Return: Always 0.
*/

listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
	unsigned int cont = 0;

	if (head == NULL)
		return (NULL);

	while (head)
	{
		if (cont == index)
		{
			return(head->n);
		}
		cont++;
	}
	return (0);
}
