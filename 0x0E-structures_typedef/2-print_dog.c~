#include <stdlib.h>
#include <stdio.h>
#include "dog.h"

/**
 * print_dog - Short description
 * @d: pointer by main
 *
 * Return None
 */

void print_dog(struct dog *d)

{
	if (d == NULL)
	{
		return;
	}
	if ((*d).name == NULL)
	{
		printf("Name: (nil)");
	}
	else
	{
		printf("Name: %s", (*d).name);
		printf("age: %f", (*d).age);
	}
	if ((*d).owner == NULL)
	{
		printf("Owner: (nil)");
	}
	else
	{
		printf("Owner: %s", (*d).owner);
	}
}
