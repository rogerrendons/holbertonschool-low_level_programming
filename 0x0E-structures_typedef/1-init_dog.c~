#include <stdlib.h>
#include <stdio.h>
#include "dog.h"

/**
 * struct dog - Short description
 * @name: Name dog
 * @age: Age dog
 * @owner: Owner dog
 *
 * Description: Longer description
 */

void init_dog(struct dog *d, char *name, float age, char *owner)

{
	if (d)
	{
		(*d).name = name;
		(*d).age = age;
		(*d).owner = owner;
	}
}
